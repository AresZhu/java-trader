<?xml version="1.0" encoding="UTF-8"?>

<root>
    <MarketDataService>
        <subscriptions>
            au1906
        </subscriptions>
    </MarketDataService>

    <TradeService>
        <account id="sim-account1" ><![CDATA[
[connectionProps]
initMoney=500000.00
commissionsFile=${trader.home}/etc/sim-account1.commissions.json
]]>
        </account>
    </TradeService>

    <TradletService>
        <!-- 定义无法自动发现需要明确加载的Tradlet实现类名 -->
        <tradlets><![CDATA[
            trader.service.tradlet.impl.StopLossTradlet
            trader.service.tradlet.impl.MACD135Tradlet
        ]]></tradlets>

        <tradletGroup id="group_au" ><![CDATA[
#This is comment
[common]
state=disabled
exchangeable=au1906
account=sim-account1

[StopLoss]

[MACD135]
playbook.stopLoss.priceSteps=4t=30s;8t=1s

        ]]></tradletGroup>
    </TradletService>

</root>
